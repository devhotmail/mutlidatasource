<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tuling.busi.mapper.OrderInfoMapper">
    <insert id="saveOrder" parameterType="com.tuling.busi.entity.OrderInfo">
        insert into order_info(order_no,user_id,create_date,product_id,product_num)
        values(#{orderNo},#{userId},#{createDate},#{productId},#{productNum})
    </insert>
</mapper>